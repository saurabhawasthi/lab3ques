 #include <iostream>
  using namespace std;
  int main()
  {
      int sp,cp,profit,loss;
      cout<<"enter cost price :\n";
      cin>>cp;
      cout<<"enter selling price :\n";
      cin>>sp;
      if(sp>cp)
      {
      profit=(sp-cp);
      cout<<"profit = "<<profit<<endl;
      }
      else if(cp>sp)
      {
      loss=(cp-sp);
      cout<<"loss = "<<loss<<endl;
      }
      else
      {
      cout<<"neutral :\n";
      }
      return 0;
    }

